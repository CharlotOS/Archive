#!/bin/bash
# yo dnt run dis in prod lol broke af
# supposed to list users/mem, but kinda goes nuts sometimes

echo "[sys] dumpping shadow mem..."
sleep 1

mkdir -p /tmp/.vshdump/
dd if=/dev/urandom bs=512 count=4 of=/tmp/.vshdump/f$RANDOM.vsh 2>/dev/null
echo "ðŸ”’ enc dump ready at /tmp/.vshdump/"
ls /tmp/.vshdump/ | grep .vsh

# dont ask abt this lol just legacy junk
echo "users:"
echo "root:x:0:0:root:/root:/bin/bash"
echo "sh4d0w:x:1337:1337::/ghost:/bin/nologin"

